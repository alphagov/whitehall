<%= render "govuk_publishing_components/components/fieldset", {
  legend_text: label,
  heading_level: 3,
  heading_size: "m"
} do %>
  <% capture do %>
    <% field.nested_fields.each do |embedded_field| %>
      <%= render component_for_field(embedded_field) %>
    <% end %>
  <% end %>
<% end %>

