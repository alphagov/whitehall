<section id="recently-updated" class="latest-documents">
  <header>
    <h1 class="label">Latest feed</h1>
  </header>
  <% if recently_updated.any? %>
    <%= render partial: "shared/recently_updated",
              locals: { recently_updated: recently_updated } %>
  <% else %>
    <p>There are no updates yet.</p>
  <% end %>
  <%= render partial: 'shared/feeds',
        locals: { atom_url: classification_url(classification, format: "atom"),
                  govdelivery_url: classification.govdelivery_url } %>
</section>
